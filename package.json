  },
  "bugs": {
    "url": "https://github.com/gravity-ui/onboarding/issues"
  },
  "homepage": "https://github.com/gravity-ui/onboarding#readme",
  "scripts": {
    "prepare": "husky install",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
