    "@commitlint/config-conventional": "^17.0.0",
    "@gravity-ui/eslint-config": "^1.0.0",
    "@gravity-ui/prettier-config": "^1.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@size-limit/esbuild-why": "^8.2.6",
